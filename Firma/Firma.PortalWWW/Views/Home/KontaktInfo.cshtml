@model IEnumerable<Firma.Data.Data.CMS.Kontakt>
@foreach (var item in Model)
{
    <div class="col-md-6 mb-4">
        <div class="bg-light p-4 rounded shadow-sm">
            <h5 class="fw-bold">@item.Biuro</h5>
            <p>@item.Adres<br>@item.Miasto, @item.KodPocztowy </p>
            <p><strong>Telefon:</strong> @item.Telefon</p>
            <p><strong>Email:</strong> @item.Email</p>
            <p><strong>Godziny otwarcia:</strong><br>@item.GodzinyPracy</p>
        </div>
    </div>
}